    "homepage": "https://github.com/koftikes/yii2-sypexgeo",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/koftikes/yii2-sypexgeo/issues"
    },
    "authors": [
        {
            "name": "Konstantin Litvinov",
            "email": "koftikes@gmail.com",
            "homepage": "http://sbsweb.ru"
        },
        {
            "name": "Pavel Agalecky",
            "email": "pavel.agalecky@gmail.com"
        }
